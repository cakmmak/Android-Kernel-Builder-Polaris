[
  {
    "kernelSource": {
      "name": "D8G",
      "repo": "https://github.com/MoeKernel/android_kernel_xiaomi_ginkgo",
      "branch": "thirteen",
      "device": "ginkgo-stock",
      "defconfig": "ginkgo-stock_defconfig"
    },
    "withKernelSU": false,
    "toolchains": [
      {
        "repo": "https://github.com/Ariden24/android-kernel-tools-clang-13",
        "branch": "clang13",
        "name": "clang13",
        "binPath": ["./bin"]
      }
    ],
    "params": {
      "ARCH": "arm64",
      "CC": "clang13/clang/host/linux-x86/clang-r428724/bin/clang",
      "externalCommand": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "clang13/linaro-gcc-4.9/aarch64-linux-gnu/bin/aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "clang13/linaro-gcc-4.9/arm-linux-gnueabi/bin/arm-linux-gnueabi-",
        "AS": "clang13/clang/host/linux-x86/clang-r428724/bin/llvm-as",
        "LD": "clang13/clang/host/linux-x86/clang-r428724/bin/ld.lld",
        "OBJCOPY": "clang13/clang/host/linux-x86/clang-r428724/bin/llvm-objcopy",
        "OBJDUMP": "clang13/clang/host/linux-x86/clang-r428724/bin/llvm-objdump",
        "STRIP": "clang13/clang/host/linux-x86/clang-r428724/bin/llvm-strip",
        "LLVM_AR": "clang13/clang/host/linux-x86/clang-r428724/bin/llvm-ar"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "repo": "https://github.com/Ariden24/AnyKernel3",
      "branch": "master"
    }
  }
]
